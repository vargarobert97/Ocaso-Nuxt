HOST=0.0.0.0
PORT=1337

# Database
DATABASE_CLIENT=sqlite
DATABASE_FILENAME=.tmp/data.db

# App Keys (generate with: openssl rand -base64 32)
APP_KEYS=your-app-key-1,your-app-key-2

# API Token Salt (generate with: openssl rand -base64 32)
API_TOKEN_SALT=your-api-token-salt

# Admin JWT Secret (generate with: openssl rand -base64 32)
ADMIN_JWT_SECRET=your-admin-jwt-secret

# JWT Secret (generate with: openssl rand -base64 32)
JWT_SECRET=your-jwt-secret

# CORS
CORS_ORIGIN=http://localhost:3000

# Vercel Blob Storage (for production)
BLOB_READ_WRITE_TOKEN=your-vercel-blob-token
VERCEL_BLOB_DOMAIN=your-vercel-blob-domain.public.blob.vercel-storage.com

# Cache Busting Webhook
CACHE_BUST_WEBHOOK_PASSWORD=your-secure-webhook-password
FRONTEND_URL=http://localhost:3000 