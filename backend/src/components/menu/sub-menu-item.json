{
  "collectionName": "components_menu_sub_menu_items",
  "info": {
    "displayName": "Sub Menu Item",
    "description": "Second level menu item"
  },
  "options": {},
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "label": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "page": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page"
    },
    "url": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "target": {
      "type": "enumeration",
      "default": "_self",
      "enum": [
        "_self",
        "_blank",
        "_parent",
        "_top"
      ]
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "isActive": {
      "type": "boolean",
      "default": true
    }
  }
}
